{
  "name": "phillarmonic/pixicato-bundle",
  "description": "A Symfony bundle for validating Brazilian PIX keys.",
  "type": "symfony-bundle",
  "keywords": ["symfony", "pix", "brazil", "cpf", "cnpj","phone","email","validation"],
  "version": "1.0.1",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Phillarmonic\\PIXicato\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phillarmonic\\PIXicato\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Phillarmonic Software",
      "email": "phillarmonicsoftware@outlook.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "symfony/framework-bundle": "^6.0|^7.0",
    "symfony/validator": "^6.0|^7.0",
    "giggsey/libphonenumber-for-php": "^8.0",
    "phillarmonic/cpf-cnpj": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0|^11.0",
    "symfony/phpunit-bridge": "^6.0|^7.0",
    "squizlabs/php_codesniffer": "*"
  },
  "scripts": {
    "test": "phpunit",
    "test-debug": "phpunit --debug"
  }
}
